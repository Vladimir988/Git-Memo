git add .                   // добавить все файлы в индекс
git rm <имя файла>          // удалить файл
git rm --cached <имя файла> // убрать файл из индекса гита
touch <имя файла>           // создай файл
git commit                  // зафиксируй состояние (сделай слепок проекта)
git log                     // лог
git reset --hard            // 